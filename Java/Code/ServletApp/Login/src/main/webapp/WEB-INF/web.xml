<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
	version="5.0">
	<display-name>Login</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<context-param>
		<param-name>url_mysql</param-name>
		<param-value>jdbc:mysql://localhost:3306/jdbc</param-value>
	</context-param>
	<context-param>
		<param-name>driver_mysql</param-name>
		<param-value>com.mysql.cj.jdbc.Driver</param-value>
	</context-param>
	<context-param>
		<param-name>pass_mysql</param-name>
		<param-value>Diep1312#</param-value>
	</context-param>
	<context-param>
		<param-name>username_mysql</param-name>
		<param-value>root</param-value>
	</context-param>
</web-app>